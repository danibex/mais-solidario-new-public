{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "#/*": ["./*"],
      "components": ["./src/components/*"],
      "styles": ["./src/styles"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/pages/index.tsx", "src/pages/_app.tsx", "src/firebase/firebaseConfig.js", "src/core/curso.ts", "src/components/home/Home.jsx", "src/contexts/AutenticacaoContext.jsx" ],
  "exclude": [
    "node_modules"
  ]
}
